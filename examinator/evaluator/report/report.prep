<html>
  <head>
    <script src="https://unpkg.com/jspdf@latest/dist/jspdf.min.js"></script>
     <script>
      window.onload = function()
      {
        var pdf = new jsPDF('p', 'pt', 'a4');
        pdf.setFontSize(18);
        pdf.fromHTML(document.getElementById('report'));

        var iframe = document.createElement('iframe');
        iframe.setAttribute('style','position:absolute; height:100%; width:100%;');
        document.body.appendChild(iframe);

        iframe.src = pdf.output('datauristring');

        var body = document.getElementById("report");
        body.style.display = "none";
      }
    </script>
  </head>
  <body>
    <div id="report">
      {{for student in students['students']}}
              <h1>Student:{{student}}</h1>
              <table>
                <thead>
                <tr>
                  <th>Subject</th><th>MaxMarks</th><th>Marks</th>
                </tr>
                </thead>
                <tbody>
                  {{for subject in students['students'][student]}}
                     <tr> <td>{{subject}}</td><td>{{students['students'][student][subject]['maxMarks']}}</td><td>{{students['students'][student][subject]['marks']}}</td></tr>
                  {{endfor}}
                </tbody>
              </table>
      {{endfor}}
    </div>
  </body>
</html>
